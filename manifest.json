{
    "name": "SmartLEAD Steins;Gate Theme",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://smartlead.hallym.ac.kr/login.php"
            ],
            "js": [
                "scripts/loginpage.js"
            ],
            "css": [
                "styles/loginpage.css"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "matches": ["https://smartlead.hallym.ac.kr/*"],
            "resources": ["images/character_bg.png"]
        }
    ]
}